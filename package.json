{
  "name": "botui-child-next",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn run lint --fix",
    "clean": "lerna clean",
    "bootstrap": "lerna bootstrap",
    "run:admin": "lerna run --stream --scope @botui/admin",
    "run:api": "lerna run --stream --scope @botui/api",
    "run:chat-components": "lerna run --stream --scope @botui/chat-components",
    "run:chat-controller": "lerna run --stream --scope @botui/chat-controller",
    "run:chat-host": "lerna run --stream --scope @botui/chat-host",
    "run:types": "lerna run --stream --scope @botui/types",
    "run:use-cors-state": "lerna run --stream --scope use-cors-state"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^0.2.34",
    "@emotion/babel-preset-css-prop": "^11.0.0",
    "@emotion/react": "^11.1.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/deep-equal": "^1.0.1",
    "@types/is-color": "^1.0.0",
    "@types/node": "^14.14.20",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-scroll": "^1.8.2",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.11",
    "ace-builds": "^1.4.12",
    "aws-amplify": "^3.3.14",
    "card-validator": "^8.0.0",
    "deep-equal": "^2.0.5",
    "fill-range": "^7.0.1",
    "formik": "2.2.6",
    "is-color": "^1.0.2",
    "jp-prefecture": "^0.2.1",
    "libphonenumber-js": "^1.9.7",
    "material-ui-color": "^0.4.6",
    "material-ui-dropzone": "^3.5.0",
    "next": "^9.5.5",
    "react": "^16.13.1",
    "react-ace": "^9.2.1",
    "react-dom": "^16.13.1",
    "react-id-generator": "^3.0.1",
    "react-loading": "^2.0.3",
    "react-nl2br": "^1.0.1",
    "react-scroll": "^1.8.1",
    "react-textarea-autosize": "^8.3.0",
    "react-use-kana": "^2.3.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3",
    "use-cors-state": "^1.1.1",
    "use-postal-jp": "^1.1.4",
    "uuid": "^8.3.2",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.7",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/*/src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
